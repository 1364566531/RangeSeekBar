<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Base.Sephiroth.Widget.RangeProgressBar" parent="Widget.AppCompat.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/abc_seekbar_track_material</item>
        <item name="android:background">#05ff0000</item>
        <item name="android:max">100</item>
        <item name="sephiroth_rpb_offset">0dp</item>
    </style>


    <style name="Base.Sephiroth.Widget.RangeSeekBar" parent="Widget.AppCompat.SeekBar">
        <item name="sephiroth_rsb_useDisabledAlpha">false</item>
        <item name="sephiroth_rpb_offset">16dp</item>
        <item name="sephiroth_rsb_thumbInset">3dp</item>
        <item name="android:max">100</item>
        <item name="sephiroth_rsb_stepSize">1</item>
        <item name="android:focusable">true</item>
        <item name="android:paddingStart">16dip</item>
        <item name="android:paddingEnd">16dip</item>
        <item name="android:progressDrawable">@drawable/sephiroth_rsb_seekbar_track_material</item>
        <!--<item name="android:background">@drawable/sephiroth_control_background_32dp_material</item>-->
        <!--<item name="android:progressDrawable">@drawable/sephiroth_rsb_range_progress_material</item>-->
    </style>
</resources>